{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@6.4.2",
      "destination": "wwwroot\\lib\\font-awesome",
      "files": [
        "css/all.min.css",
        "webfonts/*",
        "sprites/*"
      ]
    },
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot\\lib\\jquery",
      "files": [
        "jquery.slim.min.js"
      ]
    },
    {
      "library": "popper.js@2.11.8",
      "destination": "wwwroot\\lib\\popper.js",
      "files": [
        "umd/popper.min.js"
      ]
    },
    {
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot\\lib\\bootstrap",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.min.js"
      ]
    },
    {
      "library": "monaco-editor@0.44.0",
      "destination": "wwwroot\\lib\\monaco-editor"
    }
  ]
}